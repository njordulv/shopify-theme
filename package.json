{
  "devDependencies": {
    "@by-association-only/vite-plugin-shopify-clean": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "vite": "^6.3.6",
    "vite-plugin-shopify": "^4.0.2"
  },
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "shopify:dev": "shopify theme dev --theme=$SHOPIFY_THEME_ID",
    "shopify:push": "shopify theme push --theme=$SHOPIFY_THEME_ID",
    "deploy": "npm run build && npm run shopify:push",
    "dev:full": "npm-run-all --parallel dev shopify:dev"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.15.0",
    "alpinejs": "^3.15.0"
  }
}
